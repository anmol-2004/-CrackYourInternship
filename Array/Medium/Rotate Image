//Leetcode 48
//https://leetcode.com/problems/rotate-image/description/

<code>
class Solution {
public:
    void rotate(vector<vector<int>>& matrix) {
        int m=matrix.size();

        for(int i=0;i<m;i++){
            int n=matrix[i].size();
            for(int j=i;j<n;j++){
                swap(matrix[i][j], matrix[j][i]);
            }
        }
        for(int i=0;i<m;i++){
            reverse(matrix[i].begin(), matrix[i].end());
        }
    }

};
</code>
