// Leetcode 26
//https://leetcode.com/problems/remove-duplicates-from-sorted-array/

class Solution {
public:
    int removeDuplicates(vector<int>& arr) {
        int size=arr.size();

        int i = 0;
        int j = i+1;
        while(i < size && j < size){
            if(arr[i] == arr[j]){
                j++;
            }
            else{
                i++;
                swap(arr[i], arr[j]);
                j++;
            }
        }
        return i+1;
    }
};
